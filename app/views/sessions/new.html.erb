<h1>Login</h1>

<%= form_for(:session, url: login_path) do |f| %>
  <%= f.label :username %>
  <%= f.text_field :username %>
  <%= f.label :password %>
  <%= f.text_field :password %>
  <%= f.submit "Login" %>
<% end  %>

<h1>Or Create a new Account</h1>
<%= link_to "Create Account", new_user_path %>
